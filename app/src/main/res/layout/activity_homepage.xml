<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".homepage">
  
  <ImageView
      android:id="@+id/beauty_2_re"
      android:layout_width="70dp"
      android:layout_height="63dp"
      android:layout_alignParentLeft="true"
      android:layout_marginLeft="24dp"
      android:layout_marginStart="24dp"
      android:layout_marginEnd="24dp"
      android:layout_alignParentTop="true"
      android:layout_marginTop="23dp"
      android:background="@drawable/beauty"
      />
  <ImageView
      android:id="@id/searchTextLayout"
      android:layout_width="350dp"
      android:layout_height="200dp"
      android:layout_alignParentLeft="true"
      android:layout_marginLeft="24dp"
      android:layout_marginStart="24dp"
      android:layout_marginEnd="24dp"
      android:layout_alignParentTop="true"
      android:layout_marginTop="250dp"
      android:background="@drawable/nails2"
      />
  <ImageView
      android:id="@+id/beauty_3"
      android:layout_width="130dp"
      android:layout_height="110dp"
      android:layout_marginStart="24dp"
      android:layout_marginEnd="24dp"
      android:layout_alignParentLeft="true"
      android:layout_marginLeft="29dp"
      android:layout_alignParentTop="true"
      android:layout_marginTop="480dp"
      android:background="@drawable/nail3"
      />
  <TextView
      android:id="@+id/blue_pressons_tv"
      android:layout_width="110dp"
      android:layout_height="19dp"
      android:layout_marginStart="24dp"
      android:layout_marginEnd="24dp"
      android:layout_alignParentLeft="true"
      android:layout_marginLeft="24dp"
      android:layout_alignParentTop="true"
      android:layout_marginTop="600dp"
      android:text="Blue pressons nails"
      android:gravity="top"
      />
  <TextView
      android:id="@+id/amount_tv"
      android:layout_width="110dp"
      android:layout_height="19dp"
      android:layout_marginStart="24dp"
      android:layout_marginEnd="24dp"
      android:layout_alignParentLeft="true"
      android:layout_marginLeft="24dp"
      android:layout_alignParentTop="true"
      android:layout_marginTop="620dp"
      android:text="Amount $9"
      android:gravity="top"
      android:layout_below="@id/blue_pressons_tv"
      />
  <TextView
      android:id="@+id/rating_tv"
      android:layout_width="110dp"
      android:layout_height="19dp"
      android:layout_marginStart="24dp"
      android:layout_marginEnd="24dp"
      android:layout_alignParentLeft="true"
      android:layout_marginLeft="24dp"
      android:layout_alignParentTop="true"
      android:layout_marginTop="640dp"
      android:text="Rating *****"
      android:gravity="top"
      android:layout_below="@id/amount_tv"
      />
  <androidx.appcompat.widget.AppCompatButton
      android:id="@+id/category_btn"
      android:layout_width="90dp"
      android:layout_height="49dp"
      android:text="Category"
      android:layout_marginStart="24dp"
      android:layout_marginEnd="24dp"
      android:layout_alignParentLeft="true"
      android:layout_marginLeft="24dp"
      android:layout_alignParentTop="true"
      android:layout_marginTop="690dp"
      android:background="@drawable/authbutton"
      android:fontFamily="@font/cardo_regular"
      android:textColor="@color/white"
    />
  <androidx.appcompat.widget.AppCompatButton
      android:id="@+id/aboutus_btn"
      android:layout_width="90dp"
      android:layout_height="49dp"
      android:text="About Us"
      android:layout_marginStart="24dp"
      android:layout_marginEnd="24dp"
      android:layout_alignParentLeft="true"
      android:layout_centerInParent="true"
      android:layout_alignParentTop="true"
      android:layout_marginTop="690dp"
      android:background="@drawable/authbutton"
      android:fontFamily="@font/cardo_regular"
      android:textColor="@color/white"
      />

  <ImageView
      android:id="@+id/beauty_4"
      android:layout_width="130dp"
      android:layout_height="110dp"
      android:layout_marginStart="24dp"
      android:layout_marginEnd="24dp"
      android:layout_alignParentRight="true"
      android:layout_marginLeft="120dp"
      android:layout_alignParentTop="true"
      android:layout_marginTop="480dp"
      android:background="@drawable/nail4"
      />
  <TextView
      android:id="@+id/green_pressons_tv"
      android:layout_width="110dp"
      android:layout_height="19dp"
      android:layout_marginStart="24dp"
      android:layout_marginEnd="24dp"
      android:layout_alignParentRight="true"
      android:layout_marginLeft="120dp"
      android:layout_alignParentTop="true"
      android:layout_marginTop="600dp"
      android:text="Green pressons nails"
      android:gravity="top"
      />
  <TextView
      android:id="@+id/amount2_tv"
      android:layout_width="110dp"
      android:layout_height="19dp"
      android:layout_marginStart="24dp"
      android:layout_marginEnd="24dp"
      android:layout_alignParentRight="true"
      android:layout_marginLeft="120dp"
      android:layout_alignParentTop="true"
      android:layout_marginTop="620dp"
      android:text="Amount $9"
      android:gravity="top"
      android:layout_below="@id/green_pressons_tv"
      />
  <TextView
      android:id="@+id/rating2_tv"
      android:layout_width="110dp"
      android:layout_height="19dp"
      android:layout_marginStart="24dp"
      android:layout_marginEnd="24dp"
      android:layout_alignParentRight="true"
      android:layout_marginLeft="120dp"
      android:layout_alignParentTop="true"
      android:layout_marginTop="640dp"
      android:text="Amount $9"
      android:gravity="top"
      android:layout_below="@id/amount2_tv"
      />
  <androidx.appcompat.widget.AppCompatButton
      android:id="@+id/social_btn"
      android:layout_width="90dp"
      android:layout_height="49dp"
      android:text="Social"
      android:layout_marginStart="24dp"
      android:layout_marginEnd="24dp"
      android:layout_alignParentRight="true"
      android:layout_marginLeft="24dp"
      android:layout_alignParentTop="true"
      android:layout_marginTop="690dp"
      android:background="@drawable/authbutton"
      android:fontFamily="@font/cardo_regular"
      android:textColor="@color/white"
      />


  <RelativeLayout
      xmlns:android="http://schemas.android.com/apk/res/android"
      android:id="@id/searchTextLayout"
      android:layout_width="20dp"
      android:layout_height="20dp"
      android:layout_alignParentLeft="true"
      android:layout_marginLeft="269dp"
      android:layout_marginStart="300dp"
      android:background="@drawable/user"
      android:layout_alignParentTop="true"
      android:layout_marginTop="48dp"
      android:clipToOutline="true"
      />

  <RelativeLayout
      xmlns:android="http://schemas.android.com/apk/res/android"
      android:id="@+id/layer_1"
      android:layout_width="20dp"
      android:layout_height="20dp"
      android:layout_alignParentLeft="true"
      android:layout_marginLeft="269dp"
      android:layout_marginStart="350dp"
      android:background="@drawable/shoppingcart"
      android:layout_alignParentTop="true"
      android:layout_marginTop="48dp"
      android:clipToOutline="true"
      />

  <com.google.android.material.textfield.TextInputLayout
      android:id="@+id/searchTextLayout"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:hint="Search"
      android:layout_marginStart="24dp"
      android:layout_marginEnd="24dp"
      android:layout_marginTop="118dp"
      app:boxBackgroundMode="outline">
    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/search_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/custom_radius"
        android:fontFamily="@font/cardo_regular"
        android:layout_marginTop="270dp"/>


    <RelativeLayout
        android:id="@id/searchTextLayout"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="300dp"
        android:layout_marginLeft="269dp"
        android:layout_marginTop="-40dp"
        android:background="@drawable/search"
        android:clipToOutline="true" />

    <TextView
        android:id="@+id/nails_lipst"
        android:layout_width="220dp"
        android:layout_height="26dp"
        android:layout_alignParentLeft="false"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="48dp"
        android:layout_marginTop="52dp"
        android:gravity="top"
        android:text="Nails|Lipsticks|Makeup" />


  </com.google.android.material.textfield.TextInputLayout>

</RelativeLayout
   >